    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "lib": [
      "esnext"
    ],
    "types": [
      "react",
      "react-native",
      "expo-router"
    ]
  },
  "extends": "expo/tsconfig.base"
}